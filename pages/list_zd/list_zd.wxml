<!--pages/list/list.wxml-->
<import src="/utils/htmlStatus/index.wxml" />
<wxs src="../../utils/filter.wxs" module="filter" />
<view class="container">
  <template is="htmlStatus" data="{{...htmlStatus}}" />
  <view class="index_box1">
    <view class="sousuo_box">
      <input type="text" placeholder="搜索商户"  confirm-type="search" bindconfirm="sub_ss"/>
    </view>
  </view>
  
  <view wx:if="{{data_list.length==0}}" class="zanwu">暂无数据</view>
  <view class="li_box" wx:for="{{data_list}}" wx:for-item="item" bindtap="jump" data-id="{{item.id}}" data-url="{{'/pages/details/details?type='+type+'&indextype='+indextype+'&id='+item.id}}">
    <view>
      <view class="li_tit oh1">{{item.K}}</view>
      <view class="li_h2 oh1">终端号：{{item.I}}</view>
      <view class="li_h2 oh1">商户编号：{{item.J}}</view>
    </view>
    <view class="li_go">
      <image src="/static/images/li_go.jpg"></image>
    </view>
  </view>
</view>
