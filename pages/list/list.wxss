/* pages/list/list.wxss */
.li_box{
  width: 100%;
  padding: 30rpx 40rpx;
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  letter-spacing:4rpx;
  font-family: Arial, Helvetica, sans-serif
}
.li_go{
  flex: none;
  width: 50rpx;
  height: 50rpx;
}
.li_go image{
  width: 100%;
  height: 100%;
}
.li_tit{
  font-size: 32rpx;
  color: #727272;
  font-weight: bold;
  line-height: 45rpx;
  margin-bottom: 10rpx;
}
.li_h2{
  font-size: 22rpx;
  color: #3c8cf6;
  margin-bottom: 10rpx;
}
.li_add{
font-size: 22rpx;
  color: #969696;
  font-weight: bold;
  line-height: 35rpx;
}